{
  "name": "ying-tools",
  "version": "1.0.2",
  "description": "嘤之工具库",
  "keywords": [
    "嘤",
    "ying",
    "typescript",
    "tool",
    "下载github库",
    "管理切换npm源"
  ],
  "author": "JackDeng666",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/JackDeng666/ying-tools.git"
  },
  "main": "index.js",
  "type": "commonjs",
  "bin": {
    "ying": "index.js"
  },
  "scripts": {
    "dev": "npm run build:ts && npm run dev:rollup | npm run dev:ts",
    "dev:ts": "tsc -w -p tsconfig.json",
    "dev:rollup": "rollup -c -w",
    "build": "npm run build:ts && npm run build:rollup",
    "build:ts": "rm -rf lib && tsc -b tsconfig.json",
    "build:rollup": "rm -rf dist && rollup -c"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "4",
    "commander": "^11.0.0",
    "compressing": "^1.10.0",
    "inquirer": "8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@types/inquirer": "8",
    "@types/node": "^20.5.7",
    "rollup": "^3.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.2.2"
  }
}
